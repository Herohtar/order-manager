{
  "name": "order-manager",
  "version": "0.1.0",
  "description": "Order Manager Website",
  "author": "Herohtar <belac1186@gmail.com>",
  "repository": "git@github.com:Herohtar/order-manager.git",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "serve": "serve dist -p 3000",
    "deploy": "firebase deploy",
    "deploy-functions": "firebase deploy --only functions",
    "deploy-hosting": "firebase deploy --only hosting",
    "funcs:build": "babel src/functions --out-dir dist/functions",
    "funcs:install": "cd dist/functions && npm install",
    "funcs:ci": "cd dist/functions && npm ci",
    "funcs:predeploy": "npm run funcs:build && npm run funcs:ci"
  },
  "dependencies": {
    "@material-ui/core": "3.0.2",
    "@material-ui/icons": "^3.0.1",
    "firebase": "^5.4.1",
    "moment": "^2.22.2",
    "react-firebaseui": "^3.0.7",
    "react-flip-toolkit": "^4.0.6",
    "react-moment": "^0.7.9",
    "react-static": "^5.9.7",
    "recompose": "^0.28.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "eslint-config-react-tools": "^1.1.6",
    "serve": "^9.6.0"
  }
}
